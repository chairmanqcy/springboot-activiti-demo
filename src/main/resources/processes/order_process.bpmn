    <documentation>流程描述</documentation>
    <extensionElements>

    </extensionElements>
    <startEvent  id="start" name="开始节点" activiti:initiator="apply_user">
    </startEvent>
    <endEvent  id="end" name="结束节点">
    </endEvent>
    <userTask  id="apply" name="申请发布" activiti:assignee="apply_user" >
    </userTask>
    <userTask  id="approve" name="发布审批" activiti:assignee="approve_user" >
