    <sequenceFlow  id="flow4" name=" " sourceRef="approve" targetRef="gateway2">
    </sequenceFlow>
    <sequenceFlow  id="flow6" name="" sourceRef="gateway2" targetRef="end">
      <conditionExpression  xsi:type="tFormalExpression">
        <![CDATA[${ approvalStatus == 1 }]]>
      </conditionExpression>
    </sequenceFlow>
    <sequenceFlow  id="flow5" name="" sourceRef="gateway2" targetRef="apply">
      <conditionExpression  xsi:type="tFormalExpression">
        <![CDATA[${ approvalStatus == 2 }]]>
      </conditionExpression>
    </sequenceFlow>
